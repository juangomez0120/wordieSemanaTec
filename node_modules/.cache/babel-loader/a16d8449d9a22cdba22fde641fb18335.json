A,EAAA,EAAA;AACA,QAAA,EAAA,CAAA,UAAA,CAAA,OAAA,EACA,GADA,CACA;AAAA,UAAA,IAAA,EAAA;AAAA,SADA,EAEA,IAFA,CAEA,YAAA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,gCAAA;AACA,SAJA,EAKA,KALA,CAKA,UAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,KAAA,CAAA,0BAAA,EAAA,KAAA;AACA,SAPA;AAQA;AACA;AAlBA,GA3BA;AA+CA,EAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,QAAA,IAAA,GAAA,OAAA,EAAA;AACA,IAAA,kBAAA,CAAA,IAAA,EAAA,UAAA,IAAA,EAAA;AACA,UAAA,IAAA,EAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,OAHA,MAIA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,QAAA,GAAA,KAAA;AACA,QAAA,KAAA,CAAA,IAAA,CAAA,IAAA,GAAA,EAAA;AACA;AACA,KATA,CAAA;AAUA,QAAA,OAAA,GAAA;AACA,MAAA,MAAA,EAAA,KADA;AAEA,MAAA,GAAA,EAAA,0CAFA;AAGA,MAAA,MAAA,EAAA;AAAA,QAAA,MAAA,EAAA;AAAA,OAHA;AAIA,MAAA,OAAA,EAAA;AACA,2BAAA,2BADA;AAEA,0BAAA;AAFA;AAJA,KAAA;AASA,QAAA,IAAA,GAAA,IAAA;AACA,IAAA,KAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,UAAA,QAAA,EAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,MAAA,IAAA,CAAA,OAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,EAAA,UAAA;AACA,MAAA,IAAA,CAAA,GAAA,GAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,CAAA,EAAA,UAAA;AACA,KANA,EAMA,KANA,CAMA,UAAA,KAAA,EAAA;AACA,MAAA,OAAA,CAAA,KAAA,CAAA,KAAA;AACA,KARA;AAUA;AA/EA,CAAA","sourcesContent":["<template>\n  <div class=\"hello\">\n\n    <!-- Aquí va a ir todo lo del LOG IN-->\n    <div style=\"background-color: #0d6efd;\">\n      <div class=\"text-center\">\n        <button v-if=\"!authenticated\" @click=\"login\"  type=\"button\" class=\"btn btn-light\" style=\"  width:200px; height:40px; border-radius:3%;\"> <img src=\"https://img.icons8.com/color/16/000000/google-logo.png\">             Login with Google</button>\n      </div>\n    </div>\n\n    <div v-if=\"authenticated\">\n      <div class=\"container-fluid m-0 p-0\">\n        <div id=\"bar\" class=\"bg-primary p-4\">\n          <div class=\"row\">\n            <div class=\"col-7\" id=\"dailyWord\">\n              <h5 class=\"align-text-top text-white\">DailyWord</h5>\n            </div>\n            <div class=\"col\" id=\"texto\">\n              <h5 class=\"align-text-top text-white\">Mis Palabras</h5>\n            </div>\n            \n            \n            <div class=\"col\">\n              <i\n                class=\"bi-person-circle align-text-top\"\n                style=\"font-size: 2rem; color: white\"\n              ></i>\n            </div>\n\n             <div class=\"col\">\n           <!-- botón de LOGOUT-->\n              <button @click=\"logout\" type=\"button\" class=\"btn btn-outline-light\">Logout</button>\n            </div>\n\n            <div class=\"col\">\n           <!-- botón de LOGOUT-->\n              <button @click=\"createWord(palabra)\" type=\"button\" class=\"btn btn-outline-light\">DailyWord</button>\n            </div>\n\n          </div>\n        </div>\n        \n        <div id=\"landing-text\" class=\"\">\n          <div class=\"row\">\n            <p class=\"col-8 offset-3 fs-1 fw-bold\">Hola, {{ firstName }}</p>\n          </div>\n          <div>\n            <p class=\"col-8 text-muted offset-3 fs-3\">\n              You can only fail when you stop trying.\n            </p>\n\n            <!-- aqui imprimo la palabra-->\n            <p class=\"col-8 text-muted offset-3 fs-3\">{{ palabra }}</p>\n            <!-- aqui imprimo la definición-->\n            <p class=\"col-8 text-muted offset-3 fs-3\">{{ def }}</p>\n\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n\n<script>\nimport Firebase from \"../firebase.js\";\nimport { getAuth, onAuthStateChanged } from \"firebase/auth\";\nimport axios from \"axios\";\nimport \"firebase/firestore\";\nconst db = Firebase.firestore();\nexport default {\n  name: \"HelloWorld\",\n  props: {\n    msg: String,\n  },\n  data() {\n    return {\n      user: {\n        loggedIn: false,\n        data: {},\n      },\n      palabra: null,\n      def: null,\n      word: \"\"\n    };\n  },\n  computed: {\n    authenticated() {\n      return this.user.loggedIn;\n    },\n    firstName() {\n      if (this.user.data.displayName) {\n        return this.user.data.displayName.split(\" \")[0];\n      }\n      return null;\n    },\n  },\n  methods: {\n    login() {\n      Firebase.login();\n    },\n    logout() {\n      Firebase.logout();\n    },\n    createWord(word) {\n      if (word != \"\") {\n        db.collection(\"words\")\n          .add({ word: word})\n          .then(() => {\n            console.log(\"Document successfully written!\");\n          })\n          .catch((error) => {\n            console.error(\"Error writing document: \", error);\n          });\n      }\n    }\n  },\n  mounted: function() {\n      const auth = getAuth();\n      onAuthStateChanged( auth, user => {\n        if (user) {\n          this.user.loggedIn = true;\n          this.user.data = user;\n        }\n        else {\n          this.user.loggedIn = false;\n          this.user.data = {};\n        }\n      })\n      const options = {\n  method: 'GET',\n  url: 'https://wordsapiv1.p.rapidapi.com/words/',\n  params: {random: 'true'},\n  headers: {\n    'x-rapidapi-host': 'wordsapiv1.p.rapidapi.com',\n    'x-rapidapi-key': '97459d675emsh83b437ec412eba5p1ba43bjsn9d7291c09ad7'\n  }\n};\nconst self = this;\naxios.request(options).then(function (response) {\n  console.log(response.data);\n  console.log(response.data.word);\n  self.palabra = response.data.word;\n  console.log(response.data.results[0].definition);\n  self.def = response.data.results[0].definition;\n}).catch(function (error) {\n  console.error(error);\n});\n\n    }\n}\n</script>\n\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style>\n#landing-text {\n  margin: 0;\n  position: absolute;\n  top: 50%;\n  -ms-transform: translateY(-50%);\n  transform: translateY(-50%);\n}\n#texto {\n  text-align: center;\n  position: relative;\n  top: 50%;\n  -ms-transform: translateY(30%);\n  -webkit-transform: translateY(30%);\n  transform: translateY(30%);\n}\n#dailyWord {\n  position: relative;\n  top: 50%;\n  -ms-transform: translateY(30%);\n  -webkit-transform: translateY(30%);\n  transform: translateY(30%);\n}\n</style>"],"sourceRoot":"src/components"}]}